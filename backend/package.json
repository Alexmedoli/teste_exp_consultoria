{
  "name": "prisma-typescript-project",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only --exit-child src/server.ts",
    "db:migrate": "npx prisma migrate dev --name user-entity --create-only && prisma generate",
    "db:push": "npx prisma db push",
    "docs": "typedoc"
  },
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es6",
      "dom"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "prisma": "^5.15.1",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.26.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "@types/cors": "^2.8.17",
    "chart.js": "^4.4.3",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "16.4.5",
    "express": "^4.19.2",
    "express-openapi-validator": "^5.2.0",
    "logger": "^0.0.1",
    "openapi-typescript": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
